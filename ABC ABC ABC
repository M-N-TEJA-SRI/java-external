import java.util.Scanner;
class A {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        char n=sc.next().charAt(0);
        char i,j;
        for(i='A';i<=n;i++)
        {
            for(j='A';j<=n;j++)
            {
                System.out.print(j);
            }
            System.out.println();
        }
    }
}
output
C
ABC
ABC
ABC
