import java.util.Scanner;
class A {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
        int i,j;
for(i=1;i<=n;i++)
            {
                for(j=n;j>=1;j--)
                {
                System.out.print(j);
            }
            System.out.println();
        }
    }
}
output
3
321
321
321
