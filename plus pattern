import java.util.Scanner;
class A {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
      int n=5;
      int i,j;
      for(i=0;i<n;i++)
      {
          for(j=0;j<n;j++)
          {
              if(i==n/2||j==n/2)
              System.out.print("*");
              else
              System.out.print(" ");
          }
          System.out.println();
      }
    }
}
output
  *  
  *  
*****
  *  
  *  
