import java.util.Scanner;

class A {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int c=sc.nextInt();
       if(a<b+c||b<c+a||c<a+b)
       System.out.println("valid");
       else
       System.out.println("invalid");
       
    }
}
output
3 4 5
valid
