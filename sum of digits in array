import java.util.Scanner;
class A{
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
    int[] a=new int[n];
    int i,sum=0,temp,rem;
    for(i=0;i<n;i++)
    {
        a[i]=sc.nextInt();
        temp=a[i];
        while(temp!=0)
        {
            rem=temp%10;
            sum+=rem;
            temp/=10;
        }
    }
    System.out.println(sum);
    
    }
}
output
4
45 63 90 89 
44
