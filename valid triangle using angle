import java.util.Scanner;

class A {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int c=sc.nextInt();
       if(a+b+c==180)
       System.out.println("valid");
       else
       System.out.println("invalid");
       
    }
}
output
90 98 97
invalid
